# SQL Server
SA_PASSWORD=yourStrongPassword123
SQLSERVER_PORT=1433
SQLSERVER_DB=quality

# OTEL Collector
OTEL_EXPORTER_ENDPOINT=http://platform-collector:4317
OTEL_GRPC_PORT=4317
OTEL_HTTP_PORT=4318

# Azure Functions
AZURE_FUNCTIONS_PORT=8091

# Custom Receiver
CUSTOM_RECEIVER_PORT=8080
DB_URL=jdbc:sqlserver://host.docker.internal:${SQLSERVER_PORT};databaseName=${SQLSERVER_DB};encrypt=false;

# Quality Sidecar
OTEL_METRIC_EXPORT_INTERVAL=60000
OTEL_SERVICE_NAME_CDP=consuntiviDiProduzione-quality_sidecar
EXPECTATIONS_JSON_FILE_PATH=build/resources/gx_v0.1.json
