{
    "data_product_suites": [
        {
            "physical_informations": {
                "data_source_name": "data_source_v2",
                "data_asset_name": "data_asset_v2",
                "dataframe": "/app/tests/resources/v2/v2.csv"
            },
            "expectations": [
                {
                    "expectation_name": "expectGenderToBeInSet",
                    "expectation_type": "expect_column_values_to_be_in_set",
                    "kwargs": {
                        "column": "Gender",
                        "value_set": ["Male", "Female"]
                    }
                },
                {
                    "expectation_name": "expectPurchaseAmountValuesToBeOfType",
                    "expectation_type": "expect_column_values_to_be_of_type",
                    "kwargs": {
                        "column": "PurchaseAmount",
                        "type_": "float64"
                    }
                },
                {
                    "expectation_name": "expectPurchaseDateValuesToMatchRegex",
                    "expectation_type": "expect_column_values_to_match_regex",
                    "kwargs": {
                        "column": "PurchaseDate",
                        "regex": "^(\\d{4})-(\\d{2})-(\\d{2})$"
                    }
                }
            ]
        }
    ]
}
